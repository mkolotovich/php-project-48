{
    "name": "hexlet/code",
    "bin": [
        "bin/gendiff"
    ],
    "description": "diff generator",
    "autoload": {
        "files": [
            "src/Differ.php",
            "src/ReadFile.php",
            "src/Parsers.php",
            "src/MakeTree.php",
            "src/Formatters.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Plain.php",
            "src/Formatters/Json.php"
        ]
    },
    "authors": [
        {
            "name": "Mike Kolotovich"
        }
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^7.2",
        "tightenco/collect": "^9.52"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1"
    }
}
