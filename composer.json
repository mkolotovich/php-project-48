{
    "name": "hexlet/code",
    "bin": [
        "bin/gendiff"
    ],
    "description": "diff generator",
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src/"
        },
        "files": [
            "src/gendiff.php",
            "src/readFile.php",
            "src/parser.php",
            "src/makeTree.php",
            "src/formatters/index.php",
            "src/formatters/stylish.php"
        ]
    },
    "authors": [
        {
            "name": "Mike Kolotovich"
        }
    ],
    "require": {
        "docopt/docopt": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "^11.5"
    }
}
